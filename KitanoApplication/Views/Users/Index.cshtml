        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0)">@Resource.Home</a></li>
            <li class="breadcrumb-item"><a href="javascript:void(0)">@Resource.SystemManagement</a></li>
            <li class="breadcrumb-item"><a href="javascript:void(0)">@Resource.UserManagement</a></li>
            <li class="breadcrumb-item active"><a href="javascript:void(0)">@Resource.Detail</a></li>
        </ol>
    </div>
</div>
<div class="container-fluid">
    <div class="card" id="view">
        <div class="card-header bg-primary text-white" style=" font-size: 1rem; ">
            @Resource.UserManagement
        </div>
        <div class="card-body card-custom">
            <form action="javascript:void(0)">
                <div style="padding: 15px 0px;">
                    @if (Utils.IsCheckPemission("M_U", "PER_ADD") == true)
                    {
                        <a href="javascript:void(0);" onclick="openView(1,0,'@Html.Raw(Resource.Create)')" class="btn btn-info" onmouseup="setTimeout(function () { document.getElementById('FullName').focus() }, 10);"> @Resource.Create</a>
                    }

                    @*<a href="javascript:void(0);" onclick="DeleteAll()" class="btn btn-info"> @Resource.Delete</a>*@
                    <a href="javascript:void(0);" class="btn btn-info" data-toggle="collapse" data-target="#collapseSearch" aria-expanded="true" aria-controls="collapseSearch"><i class="fa fa-search" aria-hidden="true"></i>@Resource.SearchOption</a>
                </div>
                <div class="collapse " id="collapseSearch" style="margin-top:5px; margin-bottom:5px">
                    <div class="col-md-12" style=" flex-direction: row; display: inline-flex; justify-content: space-between; align-items: flex-start; width: 100%; ">
                        <div style="width:46%">
                            <div class="col-md-12 mb-3">
                                <div class="form-group row">
                                    <label class="col-form-label col-lg-3">
                                        @Resource.FullName
                                    </label>
                                    <div class="col-lg-8">
                                        <input type="text" class="form-control" id="filterFullName" name="filterFullName">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 mb-3">
                                <div class="form-group row">
                                    <label class="col-form-label col-lg-3">
                                        @Resource.Facility
                                    </label>
                                    <div class="col-lg-8">
                                        <select class="form-control" id="filterDeparment" name="filterDeparment">
                                            <option value="-1">@Resource.SelectFacility</option>
                                            <option value="1">Phòng Kế Toán</option>
                                            <option value="2">Phòng IT</option>
                                            <option value="3">Phòng Marketing</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 mb-3">
                                <div class="form-group row">
                                    <label class="col-form-label col-lg-3">
                                        @Resource.Status
                                    </label>
                                    <div class="col-lg-8">
                                        <select class="form-control" id="filterStatus" name="filterStatus">
                                            <option value="-1" selected="selected">-- @Resource.All --</option>
                                            <option value="0">@Resource.Block</option>
                                            <option value="1">@Resource.Actived</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div style="width:46%">
                            <div class="col-md-12 mb-3">
                                <div class="form-group row">
                                    <label class="col-form-label col-lg-4">
                                        @Resource.UserName
                                    </label>
                                    <div class="col-lg-8">
                                        <input type="text" class="form-control" id="filterUserName" name="filterUserName">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 mb-3">
                                <div class="form-group row">
                                    <label class="col-form-label col-lg-4">
                                        @Resource.UserType
                                    </label>
                                    <div class="col-lg-8">
                                        <select class="form-control" id="filterUserType" name="filterUserType">
                                            <option value="-1">@Resource.SelectUserType</option>
                                            <option value="1">Kiểm toán nội bộ</option>
                                            <option value="2">Đơn vị được kiểm toán</option>
                                            <option value="3">Khác</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 mb-3 btn-search-custom">
                        <div style="text-align:center; ">
                            <a href="javascript:void(0)" class="btn btn-info" onclick="onSearch()">@Resource.Search</a>
                        </div>
                    </div>
                    @*<div class="col-md-12 row btn-search-custom">
                            <div style="width: 100%; text-align: center; display: contents;">
                                <a href="javascript:void(0)" class="btn btn-info" onclick="onSearch()" style="display:flex; justify-content:center; min-width: 12%; ">@Resource.Search</a>
                            </div>
                        </div>*@
                </div>
            </form>
            <div class="table-responsive custom-table-scroll">
                <table id="usertable" class="table table-bordered table-striped table-hover dataTable">
                    <thead class="contain-header-custom m-gray ">
                        <tr>
                            <th class="text-center" scope="col" style="width:4%">STT</th>
                            <th class="text-center" scope="col" style="width:12%">@Resource.UserName</th>
                            <th class="text-center" scope="col" style="width:22%;">@Resource.FullName</th>
                            <th class="text-center" scope="col" style="width:8%;max-width:8% ">@Resource.Status</th>
                            <th class="text-center" scope="col" style="width:15%">@Resource.UserType</th>
                            <th class="text-center" scope="col" style="width:15%">@Resource.Facility</th>
                            <th class="text-center" scope="col" style="width:24%">@Resource.Option</th>
                        </tr>
                    </thead>
                    <tbody class="contain-tbody-custom">
                    </tbody>
                </table>
            </div>
            <div class="row">
                <div class="col-md-7"></div>
                <div class="col-md-5">
                    <partial name="_Pagination" />
                </div>
            </div>
        </div>
    </div>

    <div class="card" id="modify" style="display: none">
