<div class="modal fade" id="modelChangePass" tabindex="-1" role="dialog" aria-labelledby="modelChangePassLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: #544f90 !important ">
                <h5 class="modal-title" id="exampleModalLabel" style="color:white">@Resource.ChangePassword</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="frmChangePass">
                    <div class="form-group">
                        <div class="form-group row mb-3">
                            <div class="col-lg-5">
                                <label for="recipient-name" class="col-form-label">@Resource.UserName</label>
                            </div>
                            <div class="col-lg-7">
                                <input type="hidden" class="form-control" id="iduser">
                                <input disabled type="text" class="form-control" id="user_name" name="user_name" readonly="readonly">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="form-group row mb-3">
                            <div class="col-lg-5">
                                <label for="Newpassword" class="col-form-label">@Resource.NewPassword</label><span class="text-danger">*</span>
                            </div>
                            <div class="col-lg-7">
                                <input type="password" class="form-control" name="Newpassword" id="Newpassword">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="form-group row mb-3">
                            <div class="col-lg-5">
                                <label for="ConfirmNewpassword" class="col-form-label">@Resource.ConfirmNewPassword</label><span class="text-danger">*</span>
                            </div>
                            <div class="col-lg-7">
                                <input type="password" class="form-control" id="ConfirmNewpassword" name="ConfirmNewpassword">
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline" data-dismiss="modal">Đóng</button>
                <button type="button" class="btn btn-info" onclick="return $('#frmChangePass').submit();">@Resource.SaveAndClose</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="modelChangeWorkplace" tabindex="-1" role="dialog" aria-labelledby="modelChangeWorkplaceLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background: #544f90 !important ">
                <h5 class="modal-title" id="exampleModalLabel" style="color:white">@Resource.ChangeWorkPlace</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="frmChangeWorkplace">
                    <div class="form-group row mb-3">
                        <div class="col-lg-5">
                            <label for="recipient-name" class="col-form-label">@Resource.UserName</label>
                        </div>
                        <div class="col-lg-7">
                            <input type="hidden" class="form-control" id="iduser">
                            <input disabled type="text" class="form-control" id="user_name" name="user_name" readonly="readonly">
                        </div>
                    </div>
                    <div class="form-group row mb-3">
                        <div class="col-lg-5">
                            <label for="DeparmentChange" class="col-form-label">@Resource.Facility</label><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-7">
                            <select class="form-control" id="DeparmentChange" name="DeparmentChange">
                                <option value="-1">Chọn đơn vị/phòng ban</option>
                                <option value="1">Phòng Kế Toán</option>
                                <option value="2">Phòng IT</option>
                                <option value="3">Phòng Marketing</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-lg-5">
                            <label for="DateOfJoiningChange" class="col-form-label">@Resource.DateOfJoining</label><span class="text-danger">*</span>
                        </div>
                        <div class="col-lg-7">
                            <input type="text" class="form-control" id="DateOfJoiningChange" name="DateOfJoiningChange">
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline" data-dismiss="modal">Đóng</button>
                <button type="button" class="btn btn-info" onclick="return $('#frmChangeWorkplace').submit();">@Resource.SaveAndClose</button>
            </div>
        </div>
    </div>
</div>

