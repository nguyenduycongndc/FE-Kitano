                                    <select class="form-control selectpicker" id="data_source" name="data_source" onchange="changeStatusPass()">

                                        <option value="0">Kitano</option>
                                        <option value="1">LDAP</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 row" style="display: inline-flex; flex-direction: row; justify-content: end; align-items: flex-start; width: 100%;">
