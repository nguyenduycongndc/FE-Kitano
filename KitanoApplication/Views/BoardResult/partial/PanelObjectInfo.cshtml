@using RiskAssessment.Resource;
<!-- panelObjectInfo -->
<div id="panelObjectInfo" class="container tab-pane fade">
    <div class="card">
        <div class="card-body">
            <div class="form-group row">
                <label for="target_info" class="col-md-3 col-form-label">
                    @Resource.Target/@Resource.Strategy
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="target_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="mainprocess_info" class="col-md-3 col-form-label">
                    @Resource.MainProcess
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="mainprocess_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="itsystem_info" class="col-md-3 col-form-label">
                    @Resource.ITSystem
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="itsystem_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="project_info" class="col-md-3 col-form-label">
                    @Resource.Project
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="project_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="outsourcing_info" class="col-md-3 col-form-label">
                    @Resource.Outsourcing
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="outsourcing_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="supplier_info" class="col-md-3 col-form-label">
                    @Resource.Supplier
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="supplier_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="internal_info" class="col-md-3 col-form-label">
                    @Resource.InternalRegulations
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="internal_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="law_info" class="col-md-3 col-form-label">
                    @Resource.LawRegulations
                </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" id="law_info" />
                </div>
            </div>
            <div class="form-group row mt-1">
                <label for="file_attach" class="col-md-3 col-form-label">
                    @Resource.AttachFile
                </label>
                <div class="col-md-9" id="divDownloadAttach">
                    <input type="file" class="form-control" id="file_attach" />
                </div>
            </div>
            <hr />
            <div class="form-group row mt-5">
                <div class="col-md-12 text-center">
                   <button class="btn btn-info" onclick="saveScoreBoard()">@Resource.Update</button>
                </div>
            </div>
        </div>
    </div>
</div>