
<!DOCTYPE html>
<html class="h-100" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Phần mềm kiểm toán nội bộ</title>
    <link href="~/css/style.css" rel="stylesheet">
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon-k.png">
    <link rel="stylesheet" type="text/css" href="css/util.css">
    <link href="~/css/login.css" rel="stylesheet" />
    <link href="./plugins/sweetalert/css/sweetalert.css" rel="stylesheet">
    <style>
        .footer-fixed {
            background-size: 100%;
            position: fixed;
            bottom: 0px;
            width: 100%;
            height: 29px;
            text-align: center;
            background-color: rgba(0,0,0,0.6);
        }
    </style>
</head>

<body style="display: grid; zoom:96%">

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    @RenderBody()

    <div class="footer-fixed">
        <span class="copyright" style=" color: #fff; display: inline-block; font-size: 14px; margin-top: 5px;">
            2021 &copy; Tinhvan JSC. All rights reserved
        </span>
    </div>
    <!--**********************************
        Scripts
    ***********************************-->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script src="plugins/common/common.min.js"></script>
    <script src="js/custom.min.js"></script>
    <script src="js/settings.js"></script>
    <script src="js/gleek.js"></script>
    <script src="./plugins/sweetalert/js/sweetalert.min.js"></script>
    <script src="~/js/customer.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>





